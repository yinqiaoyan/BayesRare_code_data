colnames(pca_mat) <- paste0("PC_", seq_len(npc))
## ---- 1) 构造“占位”counts（1个基因 × N个细胞），且是 dgCMatrix，且无下划线 ----
# 特别注意：feature 名不能有下划线
feature_name <- "fakegene"  # 不要用 fake_gene
