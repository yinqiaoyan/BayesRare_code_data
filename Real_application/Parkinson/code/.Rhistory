## ---- 2) 创建 Seurat 对象 ----
obj <- CreateSeuratObject(counts = placeholder_counts, project = "FromPCA")
DefaultAssay(obj) <- "RNA"
## ---- 2) 把 PCA 嵌入加为 DimReduc ----------------------------------------
# embeddings 要求：行=细胞、列=维度
pca_dr <- CreateDimReducObject(
embeddings = pca_mat,
key = "PC_",
assay = DefaultAssay(obj)
)
obj[["pca"]] <- pca_dr
